<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="315.0"
                android:endColor="@*android:color/qs_color_accent_secondary"
                android:startColor="@*android:color/qs_color_accent_primary" />
            <corners
                android:bottomLeftRadius="14dp"
                android:bottomRightRadius="50dp"
                android:topLeftRadius="50dp"
                android:topRightRadius="14dp" />
        </shape>
    </item>
    <item
        android:bottom="6dp"
        android:end="4dp"
        android:start="4dp"
        android:top="6dp">
        <shape android:shape="rectangle">
            <solid android:color="@*android:color/qs_color_inactive" />
            <corners
                android:bottomLeftRadius="10dp"
                android:bottomRightRadius="46dp"
                android:topLeftRadius="46dp"
                android:topRightRadius="10dp" />
            <padding
                android:left="20dp"
                android:right="20dp" />
        </shape>
    </item>
</layer-list>
