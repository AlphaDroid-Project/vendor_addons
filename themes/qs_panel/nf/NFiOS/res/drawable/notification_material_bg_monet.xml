<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="?android:colorControlHighlight">
    <item>
        <shape>
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item
        android:end="24dp"
        android:start="24dp">
        <shape
            android:shape="rectangle"
            android:tint="@*android:color/qs_color_inactive">
            <solid android:color="#66FFFFFF" />
            <corners
                android:bottomLeftRadius="@*android:dimen/qs_corner_radius_top"
                android:bottomRightRadius="@*android:dimen/qs_corner_radius_top"
                android:topLeftRadius="@*android:dimen/qs_corner_radius_left"
                android:topRightRadius="@*android:dimen/qs_corner_radius_left" />
        </shape>
    </item>
    <item
        android:bottom="6dp"
        android:end="12dp"
        android:start="12dp">
        <shape
            android:shape="rectangle"
            android:tint="@*android:color/qs_color_inactive">
            <solid android:color="#B3FFFFFF" />
            <corners
                android:bottomLeftRadius="@*android:dimen/qs_corner_radius_top"
                android:bottomRightRadius="@*android:dimen/qs_corner_radius_top"
                android:topLeftRadius="@*android:dimen/qs_corner_radius_left"
                android:topRightRadius="@*android:dimen/qs_corner_radius_left" />
        </shape>
    </item>
    <item android:bottom="12dp">
        <shape android:shape="rectangle">
            <solid android:color="@*android:color/qs_color_inactive" />
            <corners android:radius="@*android:dimen/qs_corner_radius_top" />
        </shape>
    </item>
</ripple>