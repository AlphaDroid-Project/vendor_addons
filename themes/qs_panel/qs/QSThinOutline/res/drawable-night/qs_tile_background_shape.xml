<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape android:tintMode="src_atop">
                    <solid android:color="#1A000000" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                </shape>
            </item>
            <item
                android:bottom="2.0dp"
                android:end="2.0dp"
                android:start="2.0dp"
                android:top="2.0dp">
                <shape android:tintMode="src_atop">
                    <solid android:color="?android:colorBackgroundFloating" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_right" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:id="@*android:id/qs_state_layer">
        <shape />
    </item>
</layer-list>
