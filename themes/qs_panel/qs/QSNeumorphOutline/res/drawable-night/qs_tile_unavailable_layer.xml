<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape android:tintMode="multiply">
                    <solid android:color="@*android:drawable/color_unavailable" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                </shape>
            </item>
            <item
                android:bottom="2.0dp"
                android:end="2.0dp"
                android:start="2.0dp"
                android:top="2.0dp">
                <shape android:tintMode="multiply">
                    <gradient
                        android:angle="270.0"
                        android:endColor="#CC0F0F0F"
                        android:startColor="#CC363636" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_right" />
                </shape>
            </item>
            <item
                android:bottom="4.0dp"
                android:end="4.0dp"
                android:start="4.0dp"
                android:top="4.0dp">
                <shape android:tintMode="multiply">
                    <gradient
                        android:angle="90.0"
                        android:endColor="#CC0F0F0F"
                        android:startColor="#CC363636" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_left" />
                </shape>
            </item>
        </layer-list>
    </item>
</layer-list>
