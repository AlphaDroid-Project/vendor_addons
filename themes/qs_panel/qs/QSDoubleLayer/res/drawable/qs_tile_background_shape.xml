<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape android:tintMode="src_atop">
                    <solid android:color="#1A000000" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                </shape>
            </item>
            <item
                android:bottom="4.0dp"
                android:end="4.0dp"
                android:start="4.0dp"
                android:top="4.0dp">
                <shape android:tintMode="src_atop">
                    <solid android:color="@android:color/system_neutral2_10" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_left" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:tintMode="multiply">
                    <gradient
                        android:angle="0.0"
                        android:endColor="@*android:color/qs_color_accent_secondary"
                        android:startColor="@*android:color/qs_color_accent_primary" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                </shape>
            </item>
            <item>
                <shape android:tintMode="multiply">
                    <solid android:color="#40000000" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                </shape>
            </item>
            <item
                android:bottom="4.0dp"
                android:end="4.0dp"
                android:start="4.0dp"
                android:top="4.0dp">
                <shape android:tintMode="multiply">
                    <gradient
                        android:angle="0.0"
                        android:endColor="@android:color/system_accent1_50"
                        android:startColor="@*android:color/qs_color_accent_secondary" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_left" />
                </shape>
            </item>
        </layer-list>
    </item>
</layer-list>
