<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape>
                    <solid android:color="#1AFFFFFF" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                </shape>
            </item>
            <item
                android:width="20.0dp"
                android:height="20.0dp"
                android:drawable="@drawable/arrow_inactive"
                android:gravity="left|center"
                android:start="42.0dp" />
            <item android:gravity="left|center">
                <shape android:tintMode="src_atop">
                    <solid android:color="#1AFFFFFF" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                    <size android:width="56.0dp" />
                </shape>
            </item>
            <item android:gravity="left|center">
                <shape android:tintMode="src_atop">
                    <gradient
                        android:angle="0.0"
                        android:endColor="@android:color/system_neutral2_400"
                        android:startColor="@android:color/system_neutral2_600" />
                    <corners
                        android:bottomLeftRadius="@*android:dimen/qs_corner_radius_top"
                        android:topLeftRadius="@*android:dimen/qs_corner_radius_top" />
                    <size android:width="56.0dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</layer-list>
