<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape android:tintMode="src_atop">
                    <solid android:color="@*android:color/qs_color_inactive" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                </shape>
            </item>
            <item>
                <shape android:tintMode="src_atop">
                    <solid android:color="#1A000000" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                </shape>
            </item>
            <item
                android:bottom="4.0dp"
                android:gravity="left|center"
                android:start="4.5dp"
                android:top="4.0dp">
                <shape android:tintMode="src_atop">
                    <solid android:color="@*android:color/qs_color_inactive" />
                    <corners android:radius="@*android:dimen/qs_corner_radius_top" />
                    <size android:width="52.0dp" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:id="@*android:id/qs_state_layer">
        <shape />
    </item>
</layer-list>
