<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@null"
        android:gravity="fill_horizontal|center">
        <shape android:shape="rectangle">
            <corners android:radius="@*android:dimen/qs_corner_radius_top" />
            <size android:height="12.0dip" />
            <gradient
                android:angle="0.0"
                android:endColor="@*android:color/qs_color_accent_secondary"
                android:startColor="@*android:color/qs_color_accent_primary" />
        </shape>
    </item>
    <item android:gravity="end|center">
        <shape android:shape="rectangle">
            <corners android:radius="@*android:dimen/qs_corner_radius_top" />
            <size
                android:width="32.0dip"
                android:height="32.0dip" />
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item android:gravity="end|center">
        <shape android:shape="rectangle">
            <corners android:radius="@*android:dimen/qs_corner_radius_top" />
            <size
                android:width="32.0dip"
                android:height="18.0dip" />
            <solid android:color="@*android:color/system_accent2_10" />
        </shape>
    </item>
    <item
        android:end="2.0dip"
        android:gravity="end|center">
        <shape android:shape="rectangle">
            <corners android:radius="@*android:dimen/qs_corner_radius_right" />
            <size
                android:width="28.0dip"
                android:height="14.0dip" />
            <gradient
                android:angle="0.0"
                android:endColor="@*android:color/qs_color_accent_secondary"
                android:startColor="@*android:color/qs_color_accent_primary" />
        </shape>
    </item>
</layer-list>