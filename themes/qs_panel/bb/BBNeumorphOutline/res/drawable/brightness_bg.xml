<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_horizontal|center">
        <shape android:tintMode="multiply">
            <corners android:radius="@*android:dimen/qs_corner_radius_top" />
            <gradient
                android:angle="0.0"
                android:endColor="@*android:color/qs_color_accent_secondary"
                android:startColor="@*android:color/qs_color_accent_primary" />
            <size
                android:width="48.0dip"
                android:height="48.0dip" />
        </shape>
    </item>
    <item
        android:end="2.5dip"
        android:gravity="fill_horizontal|center"
        android:start="2.5dip">
        <shape android:tintMode="multiply">
            <corners android:radius="@*android:dimen/qs_corner_radius_right" />
            <gradient
                android:angle="270.0"
                android:endColor="#FFF0F0F0"
                android:startColor="#FFBDBDBD" />
            <size
                android:width="44.0dip"
                android:height="44.0dip" />
        </shape>
    </item>
    <item
        android:end="4.5dip"
        android:gravity="fill_horizontal|center"
        android:start="4.5dip">
        <shape android:tintMode="multiply">
            <corners android:radius="@*android:dimen/qs_corner_radius_left" />
            <gradient
                android:angle="90.0"
                android:endColor="#FFF0F0F0"
                android:startColor="#FFBDBDBD" />
            <size
                android:width="40.0dip"
                android:height="40.0dip" />
        </shape>
    </item>
</layer-list>