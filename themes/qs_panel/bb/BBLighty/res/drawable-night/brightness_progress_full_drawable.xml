<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@null"
        android:gravity="fill_horizontal|center">
        <shape android:shape="rectangle">
            <corners android:radius="@*android:dimen/qs_corner_radius_top" />
            <size android:height="48.0dip" />
            <gradient
                android:angle="315.0"
                android:endColor="@*android:color/qs_color_accent_secondary"
                android:startColor="@*android:color/qs_color_accent_primary" />
        </shape>
    </item>
    <item
        android:end="2.5dip"
        android:gravity="fill_horizontal|center"
        android:start="2.5dip">
        <shape android:shape="rectangle">
            <corners android:radius="@*android:dimen/qs_corner_radius_right" />
            <size android:height="44.0dip" />
            <gradient
                android:endColor="#B3000000"
                android:startColor="#99000000" />
        </shape>
    </item>
</layer-list>