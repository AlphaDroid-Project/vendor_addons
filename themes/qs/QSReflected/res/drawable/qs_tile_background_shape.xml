<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape android:tintMode="src_atop">
                    <solid android:color="@*android:color/system_neutral1_600" />
                    <corners android:radius="@*android:dimen/autofill_dialog_corner_radius" />
                </shape>
            </item>
            <item
                android:end="12dp"
                android:bottom="6dp"
                android:gravity="end|bottom"
                android:drawable="@drawable/ic_qstile_reflect_disabled" />
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:tintMode="multiply">
                    <gradient
                        android:angle="0.0"
                        android:endColor="@*android:color/system_accent2_200"
                        android:startColor="@*android:color/system_accent1_400" />
                    <corners android:radius="@*android:dimen/autofill_dialog_corner_radius" />
                </shape>
            </item>
            <item
                android:end="12dp"
                android:bottom="6dp"
                android:gravity="end|bottom"
                android:drawable="@drawable/ic_qstile_reflect" />
        </layer-list>
    </item>
</layer-list>
